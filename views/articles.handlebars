
  <div class="row">
          {{#each dbArticle}}
          {{#unless saved}}
    <div class="col s12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
        <a href="{{link}}">{{title}}</a>
                <form action="/articles/save/{{id}}" method="POST">
                  <input name="saved" type="hidden">
                  <button class="modal-close waves-effect waves-light btn-flat" type="submit">Save Article</button>
                </form>
        </div>
        <div class="card-action">
          <p>{{summary}}</p>
        </div>
      </div>
    </div>
    {{/unless}}
        {{/each}}
  </div>
